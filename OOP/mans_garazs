class Auto:
    def __init__(self, marka, krasa, sedvietas):
        self.marka = marka
        self.krasa = krasa
        self.sedvietas = sedvietas
    def mans_auto(self):
        print(self.marka,self.krasa,self.sedvietas)

a = input("Marka: ")
b = input("Krāsa: ")
c = input("Vietu skaits: ")

garage = Auto(a,b,c)
garage.mans_auto()
